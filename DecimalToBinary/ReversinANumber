//Reversing a number
//program for revessing a number
#include<iostream>
using namespace std;

int reverseNum(int n){
    int ans=0;
    while(n>0){
        int r=n%10;
        ans=(ans*10)+r;
        n=n/10;
    }
    return ans;
}

int main(){
    int c=123;
    int Ans=reverseNum(c);
    cout<<Ans;
}
